{
  "name": "onboard",
  "version": "1.0.0",
  "description": "Bot helps onboard new employees",
  "main": "lib/index.js",
  "engines": {
    "node": "~7.4"
  },
  "scripts": {
    "start": "./inenv.sh babel-watch src/index.js",
    "test": "ava",
    "watch": "ava --watch",
    "transpile": "babel --out-dir=lib src",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "webui-dummy-server": "babel-watch src/dummy-ui.js",
    "webui-dev": "npm run webui-dummy-server & npm run gulp",
    "gulp": "gulp",
    "gulp-build": "gulp sass"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "botkit": "file:../botkit-ciscospark",
    "pug": "^2.0.0-beta11"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.8",
    "babel-watch": "^2.0.5",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-load-plugins": "^1.1.0",
    "gulp-sass": "^2.1.0",
    "main-bower-files": "^2.13.1",
    "node-sass": "^4.5.0"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/setup"
    ],
    "babel": "inherit"
  }
}

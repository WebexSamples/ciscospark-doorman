{
  "name": "onboard",
  "version": "1.0.0",
  "description": "Bot helps onboard new employees",
  "main": "lib/index.js",
  "engines": {
    "node": "~7.4"
  },
  "scripts": {
    "prod": "node lib/index.js",
    "start": "concurrently  'npm run server-dev'  'gulp'",
    "server-dev": "./inenv.sh babel-watch src/index.js",
    "ngrok": "ngrok http 3000 -subdomain=doorman-dev",
    "test": "ava",
    "watch": "ava --watch",
    "transpile": "babel --out-dir=lib src",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "webui-dummy-server": "babel-watch src/dummy-ui.js",
    "webui-dev": "concurrently  'npm run webui-dummy-server'  'gulp'",
    "build": "gulp build",
    "gulp": "gulp",
    "bower": "bower",
    "postinstall": "npm rebuild node-sass && npm run bower install && npm run gulp build",
    "heroku-postbuild": "npm run transpile"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "botkit": "https://cl.ly/0m2I093Z3A3k/promptworks-botkit-ciscospark-5ef6eb43d0edc6350ef04a2ba5da2a866c3b8506.tar.gz",
    "pug": "^2.0.0-beta11",

    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.8",
    "babel-watch": "^2.0.5",
    "bower": "^1.8.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-load-plugins": "^1.1.0",
    "gulp-sass": "^2.1.0",
    "main-bower-files": "^2.13.1",
    "node-sass": "^4.5.0",

    "ava": "^0.17.0",
    "concurrently": "^3.3.0",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/setup"
    ],
    "babel": "inherit"
  }
}
